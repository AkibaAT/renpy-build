{
  "files.exclude": {
    "tmp": true,
    "**/__pycache__": true,
    "**/__pycache__.*": true,
    "renpy": true,
    "pygame_sdl2": true,
    "renios/prototype/prebuilt": true,
    "renpyweb/build": true,
    "renpyweb/cache": true,
    "renpyweb/pygame_sdl2": true,
    "renpyweb/renpy": true
  },
  "files.watcherExclude": {
    "**/__pycache__": true,
    "**/__pycache__.*": true,
    "**/pygame_sdl2/**": true,
    "**/python-emscripten/**": true,
    "**/renios/prototype/prebuilt/**": true,
    "**/renpy/**": true,
    "**/renpyweb/build/**": true,
    "**/renpyweb/cache/**": true,
    "**/tmp/**": true
  },
  "files.associations": {
    "algorithm": "c",
    "string": "c",
    "*.tcc": "c"
  }
}